{
  "name": "nuxt-module-auth0",
  "version": "0.0.1",
  "description": "Module for integrating auth0.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Daniel Staudigel"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/breadwallet/nuxt-module-auth0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "jest": {
